<UserControl
    x:Class="PageNavigationApp.Views.Home"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:PageNavigationApp.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="600"
    d:DesignWidth="622"
    Style="{StaticResource Page_Style}"
    mc:Ignorable="d">

    <Border CornerRadius="0 20 20 0">
        <Grid Style="{StaticResource NameBase}">
            <TextBlock Style="{StaticResource TextOne}" />
            <Border Style="{StaticResource TextBorder}">
                <TextBlock Style="{StaticResource TextTwo}" />
            </Border>
        </Grid>
    </Border>
</UserControl>